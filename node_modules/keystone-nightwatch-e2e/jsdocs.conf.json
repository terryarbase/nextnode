{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [
            "README.md",
            "lib/src/pageObjects"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./out/",
        "recurse": true
    },
    "docdash": {
        "static": false,
        "sort": true
    }
}